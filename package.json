{
  "name": "@kennedyrose/webflow-dev",
  "version": "1.0.0",
  "author": "Kennedy Rose <kennedyianrose@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "lerna run dev --parallel",
    "build": "npm run build:js && npm run move-files && npm run build:api",
    "build:api": "cd packages/api && npm run build",
    "build:js": "cd packages/js && npm run build",
    "move-files": "mkdir -p packages/api/public/js && cp -r packages/js/dist/* packages/api/public/js",
    "start": "cd packages/generator && npm run start",
    "stop": "sudo killall -9 node",
    "version": "lerna version",
    "test": "echo 'no test'"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "del-cli": "^4.0.0",
    "dotenv-cli": "^4.0.0",
    "lerna": "^4.0.0"
  }
}
