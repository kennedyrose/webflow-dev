{
  "name": "@webflow-dev/app",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "yarn run move-files && astro build",
    "build:js": "rollup src/js/script.js --format iife --file dist/bundle.js",
    "x:dev": "export VITE_API=http://localhost:8080 && vite build --watch",
    "x:build": "export VITE_API=\"${VERCEL_URL:-${URL:-http://localhost:8080}}\" && vite build",
    "move-files": "mkdir -p public/js && cp -r ../js/dist/* public/js",
    "preview": "astro preview",
    "astro": "astro"
  },
  "dependencies": {
    "@astrojs/vercel": "^3.2.2",
    "astro": "^2.2.0"
  }
}
